{
  "name": "modal-popup",
  "version": "1.0.0",
  "description": "A react-redux modal",
  "main": "lib/index.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/scss/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss/ -o src/styles/ --watch --recursive",
    "build-package:css": "node-sass-chokidar src/styles/scss/ -o lib/styles/",
    "build-package:js": "babel src/ --out-dir lib",
    "build": "npm-run-all -p build-package:css build-package:js",
    "clean": "rimraf dist lib",
    "start-js": "webpack-dev-server",
    "dev": "npm-run-all -p watch-css start-js",
    "postinstall": "npm run build",
    "build:demo": "NODE_ENV=production npm run clean && webpack -p",
    "deploy": "npm run build:demo && git subtree push --prefix dist origin gh-pages"
  },
  "keywords": [
    "React",
    "Redux",
    "Modal",
    "Lightbox",
    "popup"
  ],
  "author": "MoneySavingExpert.com",
  "license": "ISC",
  "dependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "lorem-ipsum": "^1.0.4",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
